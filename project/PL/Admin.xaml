<Window x:Class="PL.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="Admin" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnShowProductList" Click="ShowProductList_Click" Content="view product list"  Margin="587,329,0,0" Style="{StaticResource BtnStyle}"/>
        <Button x:Name="btnShowOrderList" Click="ShowOrderList_Click" Content="view order list"  Margin="404,329,0,0" Style="{StaticResource BtnStyle}"/>
    </Grid>
</Window>
